{
    "$schema": "https://dl.viam.dev/module.schema.json",
    "module_id": "erh:viam-stereo-camera",
    "visibility": "public",
    "url": "https://github.com/erh/viam-stereo-camera",
    "description": "stereo-camera",
    "models": [
        {
            "api": "rdk:component:camera",
            "model": "erh:viam-stereo-camera:stereo-camera",
            "short_description": "2 images to pcd",
            "markdown_link": "README.md#model-erhviam-stereo-camerastereo-camera"
        },
        {
            "api": "rdk:component:movement_sensor",
            "model": "erh:viam-stereo-camera:flow-movement-sensor",
            "short_description": "optical flow",
            "markdown_link": "README.md#flow-movement-sensor"
        }
    ],
    "entrypoint": "bin/viam-stereo-camera",
    "first_run": "",
    "build": {
        "build": "make module.tar.gz",
        "setup": "make setup",
        "path": "bin/module.tar.gz",
        "arch": [
            "linux/amd64",
            "linux/arm64"
        ]
    }
}
